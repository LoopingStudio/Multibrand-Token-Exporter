{
  "name": "multibrand-token-exporter",
  "version": "1.0.0",
  "description": "Export Figma design tokens with multi-brand support to JSON",
  "main": "dist/code.js",
  "scripts": {
    "build": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es6 --minify",
    "watch": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es6 --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.84.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.3"
  }
}
